    "cross-env": "6.0.3",
    "husky": "4.3.8",
    "jest-junit-reporter": "1.1.0",
    "react": "16.12.0",
    "sass": "1.25.0",
    "ts-node": "8.1.0",
    "tslib": "1.9.3",
    "typescript": "3.9.7"
